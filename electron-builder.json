{
  "productName": "Media Dashboard",
  "directories": {
    "output": "release/"
  },
    "files": [
        "**/*",
        "!**/*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/",
        "!e2e/",
        "!hooks/",
        "!angular.json",
        "!_config.yml",
        "!karma.conf.js",
        "!tsconfig.json",
        "!tslint.json"
    ],
  "extraFiles": [
    "trayTemplate@2x.png"
  ],
  "publish": {
    "provider": "github",
    "token": ""
  },
  "win": {
    "icon": "dist/assets/icons",
    "target": [
      "nsis",
      "zip"
    ],
    "publisherName": "ArgonautDevelopments",
    "certificateFile": "private/ArgonautDevelopments.pfx",
    "certificatePassword": ""
  },
  "mac": {
    "icon": "dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "category": "Utility",
    "target": [
      "tar.gz",
      "deb"
    ]
  }
}
